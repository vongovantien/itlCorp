{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "eFMS-Web": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/eFMS-Web",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/manifest.json"
                        ],
                        "styles": [
                            "src/assets/style-share/style.scss",
                            "node_modules/font-awesome/less/font-awesome.less",
                            "src/assets/external-libraries/css/editor.pkgd.min.css",
                            "src/assets/external-libraries/css/style-editor.min.css"
                        ],
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "src/assets/external-libraries/js/popper.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.min.js",
                            "src/assets/external-libraries/js/editor.pkgd.min.js"
                        ],
                        "serviceWorker": true
                    },
                    "configurations": {
                        "design": {},
                        "local": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.local.ts"
                                }
                            ]
                        },
                        "vi-dev": {
                            "fileReplacements": [
                                {
                                    "replace": "src/languages/language.en.ts",
                                    "with": "src/languages/language.vi.ts"
                                }
                            ]
                        },
                        "test": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.test.ts"
                                }
                            ]
                        },
                        "test-vi": {
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "baseHref": "/vi/",
                            "outputPath": "dist/test/eFMS-vi/",
                            "i18nFile": "src/src/locale/messages.vi.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "vi",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.test.ts"
                                },
                                {
                                    "replace": "src/languages/language.en.ts",
                                    "with": "src/languages/language.vi.ts"
                                }
                            ],
                            "serviceWorker": true
                        },
                        "staging-vi": {
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": true,
                            "extractCss": true,
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "baseHref": "/vi/",
                            "outputPath": "dist/staging/eFMS-vi/",
                            "i18nFile": "src/src/locale/messages.vi.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "vi",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                },
                                {
                                    "replace": "src/languages/language.en.ts",
                                    "with": "src/languages/language.vi.ts"
                                }
                            ],
                            "serviceWorker": true
                        },
                        "test-en": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "baseHref": "/en/",
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "outputPath": "dist/test/eFMS-en/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.test.ts"
                                }
                            ],
                            "serviceWorker": false,
                            "budgets": [
                                {
                                    "type": "bundle",
                                    "name": "vendor",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        },
                        "staging-en": {
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": true,
                            "extractCss": true,
                            "baseHref": "/en/",
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "outputPath": "dist/staging/eFMS-en/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                }
                            ],
                            "serviceWorker": false,
                            "budgets": [
                                {
                                    "type": "bundle",
                                    "name": "vendor",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        },
                        "staging-debug": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.staging.ts"
                                }
                            ]
                        },
                        "uat-debug": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.uat.ts"
                                }
                            ]
                        },
                        "uat-en": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "baseHref": "/en/",
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "outputPath": "dist/uat/eFMS-en/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.uat.ts"
                                }
                            ],
                            "serviceWorker": false,
                            "budgets": [
                                {
                                    "type": "bundle",
                                    "name": "vendor",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        },
                        "uat-vi": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "baseHref": "/vi/",
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "outputPath": "dist/uat/eFMS-vi/",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.uat.ts"
                                }
                            ],
                            "serviceWorker": true,
                            "budgets": [
                                {
                                    "type": "bundle",
                                    "name": "vendor",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ]
                        },
                        "prod-en": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "baseHref": "/en/",
                            "outputPath": "dist/prod/eFMS-en/",
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ],
                            "serviceWorker": true
                        },
                        "prod-vi": {
                            "baseHref": "/vi/",
                            "outputPath": "dist/prod/eFMS-vi/",
                            "i18nFile": "src/src/locale/messages.vi.xlf",
                            "i18nFormat": "xlf",
                            "i18nLocale": "vi",
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                },
                                {
                                    "replace": "src/languages/language.en.ts",
                                    "with": "src/languages/language.vi.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ],
                            "serviceWorker": true
                        },
                        "prod-debug": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "sourceMap": false,
                    "options": {
                        "browserTarget": "eFMS-Web:build"
                    },
                    "configurations": {
                        "test": {
                            "browserTarget": "eFMS-Web:build"
                        },
                        "stag": {
                            "browserTarget": "eFMS-Web:build:staging-debug"
                        },
                        "vi": {
                            "browserTarget": "eFMS-Web:build:vi"
                        },
                        "vi-dev": {
                            "browserTarget": "eFMS-Web:build:vi-dev"
                        },
                        "en": {
                            "browserTarget": "eFMS-Web:build:en"
                        },
                        "design": {
                            "browserTarget": "eFMS-Web:build:design"
                        },
                        "local": {
                            "browserTarget": "eFMS-Web:build:local"
                        },
                        "uat": {
                            "browserTarget": "eFMS-Web:build:uat-debug"
                        },
                        "prod": {
                            "browserTarget": "eFMS-Web:build:prod-debug"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "eFMS-Web:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "src/styles.sass"
                        ],
                        "scripts": [],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/manifest.json"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "eFMS-Web-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "eFMS-Web:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "eFMS-Web:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "eFMS-Web"
}